<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.liu.weathermail.dao.DailyDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.liu.weathermail.entity.DailyEntity" id="dailyMap">
        <result property="id" column="id"/>
        <result property="fxDate" column="fx_date"/>
        <result property="tempMax" column="temp_max"/>
        <result property="tempMin" column="temp_min"/>
        <result property="textDay" column="text_day"/>
        <result property="winddirday" column="windDirDay"/>
        <result property="windscaleday" column="windScaleDay"/>
        <result property="addtime" column="addTime"/>
        <result property="updatetime" column="updateTime"/>
        <result property="remark" column="remark"/>
        <result property="unknown" column="unknown"/>
        <result property="cityCode" column="city_code"/>
    </resultMap>


</mapper>